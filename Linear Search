#include<stdio.h>
void main() {
	int n,i, pos=-1,key,flag=0;
	int arr[100];
	scanf("%d",&n);
	for(i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	
	
	scanf("%d",&key);
	
	for(i=0;i<n;i++){
		
	
	if(key==arr[i]){
		flag = 1;
		pos = i;
		break;
	}
	
	
	
	
	
	
	
	}
	if (flag == 1) {
		printf("found at position %d\n", pos);
	} else {
		printf("%d not found\n", key);
	}
}
